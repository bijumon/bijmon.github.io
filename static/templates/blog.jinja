
{% extends "static/templates/default.jinja" %}

{{ site }}

{{ posts }}

{% block content %}
    {% for post in posts.posts %}
    <article>
        <h2>
            <a href="{{ post.url }}">{{ post.title }}</a>
        </h2>
        {{ post.content }}
        <hr/>
    </article>
    {% endfor %}
{% endblock content %}
